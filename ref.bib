  author                     = {Wang, Xue and Zhai, Haowei and Qie, Boyu and Cheng, Qian and Li, Aijun and Borovilas, James and Xu, Bingqing and Shi, Changmin and Jin, Tianwei and Liao, Xiangbiao and Li, Yibin and He, Xiaodong and Du, Shanyi and Fu, Yanke and Dontigny, Martin and Zaghib, Karim and Yang, Yuan},

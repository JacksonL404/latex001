@Article{WOS:000472691800087,
  author                     = {Wang, Shengsen and Zhao, Mingyue and Zhou, Min and Li, Yuncong C. and Wang, Jun and Gao, Bin and Sato, Shinjiro and Feng, Ke and Yin, Weiqin and Igalavithana, Avanthi Deshani and Oleszczuk, Patryk and Wang, Xiaozhi and Ok, Yong Sik},
  journal                    = {JOURNAL OF HAZARDOUS MATERIALS},
  title                      = {Biochar-supported nZVI (nZVI/BC) for contaminant removal from soil and water: A critical review},
  year                       = {2019},
  issn                       = {0304-3894},
  month                      = {JUL 5},
  pages                      = {820-834},
  volume                     = {373},
  abstract                   = {The promising characteristics of nanoscale zero-valent iron (nZVI) have
   not been fully exploited owing to intrinsic limitations. Carbon-enriched
   biochar (BC) has been widely used to overcome the limitations of nZVI
   and improve its reaction with environmental pollutants. This work
   reviews the preparation of nZVI/BC nanocomposites; the effects of BC as
   a supporting matrix on the nZVI crystallite size, dispersion, and
   oxidation and electron transfer capacity; and its interaction mechanisms
   with contaminants. The literature review suggests that the properties
   and preparation conditions of BC (e.g., pore structure, functional
   groups, feedstock composition, and pyrogenic temperature) play important
   roles in the manipulation of nZVI properties. This review discusses the
   interactions of nZVI/BC composites with heavy metals, nitrates, and
   organic compounds in soil and water. Overall, BC contributes to the
   removal of contaminants because it can attenuate contaminants on the
   surface of nZVI/BC; it also enhances electron transfer from nZVI to
   target contaminants owing to its good electrical conductivity and
   improves the crystallite size and dispersion of nZVI. This review is
   intended to provide insights into methods of optimizing nZVI/BC
   synthesis and maximizing the efficiency of nZVI in environmental
   cleanup.},

@Article{WOS:000598735100005,
  author                     = {Lashgari, Seyed Mohammad and Yari, Hossain and Mahdavian, Mohammad and Ramezanzadeh, Bahram and Bahlakeh, Ghasem and Ramezanzadeh, Mohammad},
  journal                    = {JOURNAL OF HAZARDOUS MATERIALS},
  title                      = {Synthesis of graphene oxide nanosheets decorated by nanoporous zeolite-imidazole (ZIF-67) based metal-organic framework with controlled-release corrosion inhibitor performance: Experimental and detailed DFT-D theoretical explorations},
  year                       = {2021},
  issn                       = {0304-3894},
  month                      = {FEB 15},
  number                     = {A},
  volume                     = {404},
  abstract                   = {For the first time, the zeolite-imidazole (ZIF-67) framework, a new
   subfamily of metal-organic frameworks (MOFs), is synthesized on the
   graphene oxide (GO) platform. Co2+ (as a central atom) and
   2-methylimidazole (as organic ligands) were assembled to fabricate
   ZIF-67/GO NPs for providing epoxy-based anti-corrosion coatings with

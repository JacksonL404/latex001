@Article{WOS:000566663800006,
  author                     = {Kalaj, Mark and Bentz, Kyle C. and Ayala, Jr., Sergio and Palomba, Joseph M. and Barcus, Kyle S. and Katayama, Yuji and Cohen, Seth M.},
  journal                    = {CHEMICAL REVIEWS},
  title                      = {MOF-Polymer Hybrid Materials: From Simple Composites to Tailored Architectures},
  year                       = {2020},
  issn                       = {0009-2665},

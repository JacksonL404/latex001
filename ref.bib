@article{ WOS:000336415700006,
Author = {Barta, Katalin and Ford, Peter C.},
Title = {Catalytic Conversion of Nonfood Woody Biomass Solids to Organic Liquids},
Journal = {ACCOUNTS OF CHEMICAL RESEARCH},
Year = {2014},
Volume = {47},

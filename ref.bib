@Article{WOS:000821071600009,
  author                     = {Liu, Jiahao and Shen, Liguo and Lin, Hongjun and Huang, Zhengyi and Hong, Huachang and Chen, Cheng},
  journal                    = {JOURNAL OF COLLOID AND INTERFACE SCIENCE},
  title                      = {Preparation of Ni@UiO-66 incorporated polyethersulfone (PES) membrane by magnetic field assisted strategy to improve permeability and photocatalytic self-cleaning ability},
  year                       = {2022},
  issn                       = {0021-9797},
  month                      = {JUL 15},
  pages                      = {483-495},
  volume                     = {618},
  abstract                   = {Metal-organic frameworks (MOFs) have been considered as promising
   nanofillers to fabricate mixed matrix membranes for water treatment.
   However, manipulating distribution of MOFs nanoparticles in the membrane
   matrix remains a great challenge. In this study, UiO-66 was firstly
   coated by magnetic Ni via an in-situ reduction reaction, and then
   incorporated into polyethersulfone (PES) membrane matrix to prepare
   PES-Ni@UiO-66 membrane. The magnetic Ni allowed to manipulate the

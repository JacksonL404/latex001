@article{ WOS:000416878100108,
Author = {Wang, Xiujuan and Chen, Kai and Wang, Gang and Liu, Xiaojie and Wang,
   Hui},
Title = {Rational Design of Three-Dimensional Graphene Encapsulated with Hollow
   FeP@Carbon Nanocomposite as Outstanding Anode Material for Lithium Ion
   and Sodium Ion Batteries},

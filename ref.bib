@Article{WOS:000346430100013,
  author                     = {Rodenas, Tania and Luz, Ignacio and Prieto, Gonzalo and Seoane, Beatriz and Miro, Hozanna and Corma, Avelino and Kapteijn, Freek and Llabres i Xamena, Francesc X. and Gascon, Jorge},
  journal                    = {NATURE MATERIALS},
  title                      = {Metal-organic framework nanosheets in polymer composite materials for gas separation},
  year                       = {2015},
  issn                       = {1476-1122},
